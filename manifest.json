{
  "manifest_version": 2,
 
  "name": "TF2 Trade Enhance",
  "version": "0.0.2.1",
  "description": "Make TF2 trading sites easier to browse",
 
  "content_scripts": [{
    "css": [
      "styles/fontawesome.min.css",
		  "styles/styles.css"
	  ],
    "js": [
      "setup.js",
      "ScrapTF/contentScrapTF.js",
      "BackpackTF/contentBackpackTF.js",
      "Steam/contentSteam.js",
      "content.js"
	  ],
    "matches": [
      "https://scrap.tf/*",
      "https://backpack.tf/*",
      "https://steamcommunity.com/*/*/inventory",
      "https://steamcommunity.com/tradeoffer/*"
    ]
  }],
  "web_accessible_resources": ["ScrapTF/useScrapTF.js"]
}